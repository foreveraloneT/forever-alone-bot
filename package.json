{
  "name": "foreveralone-bot",
  "version": "1.0.0",
  "description": "LINE bot for learning",
  "main": "app.js",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "start": "nodemon src/app.js --exec babel-node -e js",
    "clean": "rm -rf dist",
    "build": "npm run clean && babel src --out-dir dist --copy-files -s",
    "serve": "node dist/app.js",
    "heroku-postbuild": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/foreveraloneT/forever-alone-bot"
  },
  "keywords": [
    "LINE",
    "bot"
  ],
  "author": "Sutee S.",
  "license": "ISC",
  "dependencies": {
    "@line/bot-sdk": "^6.8.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "nodemon": "^1.19.1"
  }
}
